{
  "name": "discordbot",
  "version": "2.3.0",
  "description": "hobby project to experiment with discord.js and mongodb. This bot has a lot of different features with no direct goal.",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "ts-node-dev src/index.ts",
    "start:prod": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc --project tsconfig.json",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ramon96/DiscordBot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "expiriment",
    "discord.js"
  ],
  "author": "Ramon Meijers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ramon96/DiscordBot/issues"
  },
  "homepage": "https://github.com/Ramon96/DiscordBot#readme",
  "dependencies": {
    "cron": "^3.1.5",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "glob": "^7.2.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.6.3",
    "node-fetch": "^2.7.0",
    "osrs-json-hiscores": "2.17.0",
    "quickchart-js": "^3.1.3",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/cron": "^2.4.0",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.200",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.8.9",
    "@types/node-fetch": "^2.6.11",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
